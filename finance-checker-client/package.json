{
  "name": "finance-checker",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "deploy": "sh deploy.sh"
  },
  "dependencies": {
    "chart.js": "^2.7.3",
    "element-ui": "^2.4.11",
    "google-palette": "^1.1.0",
    "jquery": "^3.3.1",
    "material-design-icons": "^3.0.1",
    "npm": "^6.5.0",
    "string-similarity": "^3.0.0",
    "vue": "^2.5.17",
    "vue-chartjs": "^3.4.0",
    "vue-js-modal": "^1.3.28",
    "vue-lazyload": "^1.2.6",
    "vue-moment": "^4.0.0",
    "vue-router": "^3.0.2",
    "vuex": "^3.1.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.1.1",
    "@vue/cli-plugin-eslint": "^3.1.1",
    "@vue/cli-service": "^3.1.1",
    "babel-eslint": "^10.0.1",
    "element-theme-chalk": "git+https://github.com/ElementUI/theme-chalk.git",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0-0",
    "node-sass": "^4.11.0",
    "push-dir": "^0.4.1",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "vue-template-compiler": "^2.5.17"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "no-fallthrough": [
        "error",
        {
          "commentPattern": "break[\\s\\w]*omitted"
        }
      ],
      "complexity": [
        "warn",
        {
          "max": 8
        }
      ]
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
