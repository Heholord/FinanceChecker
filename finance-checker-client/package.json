{
  "name": "finance-checker",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "deploy": "sh deploy.sh"
  },
  "dependencies": {
    "chart.js": "^2.9.4",
    "css-loader": "^5.0.1",
    "element-ui": "^2.14.1",
    "google-palette": "^1.1.0",
    "jquery": "^3.5.1",
    "material-design-icons": "^3.0.1",
    "moment": "^2.29.1",
    "npm": "^6.14.10",
    "string-similarity": "^4.0.3",
    "vue": "^2.6.12",
    "vue-chartjs": "^3.5.1",
    "vue-js-modal": "^1.3.35",
    "vue-lazyload": "^1.3.3",
    "vue-router": "^3.4.9",
    "vue-tour": "^1.5.0",
    "vuex": "^3.6.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.12.1",
    "@vue/cli-plugin-eslint": "^3.1.1",
    "@vue/cli-service": "^3.12.1",
    "babel-eslint": "^10.1.0",
    "element-theme-chalk": "git+https://github.com/ElementUI/theme-chalk.git",
    "eslint": "^7.17.0",
    "eslint-plugin-vue": "^7.4.0",
    "node-sass": "^6.0.1",
    "push-dir": "^0.4.1",
    "sass-loader": "^10.1.1",
    "style-loader": "^2.0.0",
    "vue-template-compiler": "^2.6.12"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "no-fallthrough": [
        "error",
        {
          "commentPattern": "break[\\s\\w]*omitted"
        }
      ],
      "complexity": [
        "warn",
        {
          "max": 8
        }
      ]
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
